{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --build-optimizer",
    "prod:stats": "ng build --prod --aot --build-optimizer --stats-json",
    "prod:details": "webpack-bundle-analyzer ./dist/client/stats.json",
    "deploy:firebase": "firebase deploy",
    "test": "ng test",
    "format": "prettier --single-quote --trailing-comma all --print-width 120 --arrow-parens always --tab-width 4 --write",
    "tslint": "tslint \"src/**/*.ts\"",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "lint": "ng lint \"src/**/*.ts\"",
    "update": "ng update @angular/cli @angular/core",
    "update:cli": "ng update @angular/cli",
    "update:core": "ng update @angular/core",
    "e2e": "ng e2e",
    "resources": "node resources/generate-resources.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run format",
      "npm run tslint",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run stylelint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ofcyln/currency-exchange.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@ngx-loading-bar/http-client": "^2.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "4.3.1",
    "core-js": "^2.6.9",
    "currency-symbol-map": "^4.0.4",
    "rxjs": "^6.5.4",
    "rxjs-tslint": "^0.1.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.14.13",
    "angular-spec-generator": "^1.1.0",
    "codelyzer": "^5.0.1",
    "firebase-tools": "^6.12.0",
    "husky": "^0.14.3",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-trx-reporter": "^0.4.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "puppeteer": "^1.18.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
